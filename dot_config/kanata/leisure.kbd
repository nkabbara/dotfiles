;; Config for 12" MacBook with physical f keys. This config allows for brightness etc to still work.
(defsrc
  esc            f1    f2    f3    f4    f5    f6    f7    f8    f9    f10   f11   f12
  Backquote      1    2    3    4    5       6    7    8    9    0       -    =    bspc
  tab            q    w    e    r    t       y    u    i    o    p       [    ]    \ 
  caps           a    s    d    f    g       h    j    k    l    ;       '    ret 
  lsft           z    x    c    v    b       n    m    ,    .    /       rsft
  fn  lctl      lalt lmet          spc            rmet ralt
) 

(defvar
  ;; Note: consider using different time values for your different fingers.
  ;; For example, your pinkies might be slower to release keys and index
  ;; fingers faster.
  tap-time 200
  hold-time 200 
  hold-time-slow 300 
)

(defalias
  meh  (multi ctl alt sft)
  tmeh (tap-hold $tap-time $hold-time spc @meh)
   l_cmd (layer-toggle cmd)
   l_sys1 (layer-toggle sys1)
   l_sys2 (layer-toggle sys2)
   l_num (layer-toggle num)
   s (tap-hold $tap-time $hold-time s @l_cmd)
   h (tap-hold $tap-time $hold-time h @l_cmd)
   t (tap-hold $tap-time $hold-time t @l_sys1)
   a (tap-hold $tap-time $hold-time-slow a @l_sys1)
   r (tap-hold $tap-time $hold-time r @l_sys2)
   e (tap-hold $tap-time $hold-time e @l_sys2)
   n (tap-hold $tap-time $hold-time-slow n @l_num)
   i (tap-hold $tap-time $hold-time-slow i @l_num)
   x (tap-hold $tap-time $hold-time-slow x lctl)
   m (tap-hold $tap-time $hold-time m lalt)
   . (tap-hold $tap-time $hold-time-slow . lctl)
   , (tap-hold $tap-time $hold-time , lalt)
   q (tap-hold $tap-time $hold-time q lsft)
   / (tap-hold $tap-time $hold-time / rsft)
   c (tap-hold $tap-time $hold-time c lmeta)
   p (tap-hold $tap-time $hold-time p rmeta)
   d_h (tap-hold $tap-time $hold-time del home)
   p_e (tap-hold $tap-time $hold-time M-[ end)
   m_v (tap-hold $tap-time $hold-time M-v S-M-v)
   c_uz_r (chord underscore r)
   c_uz_t (chord underscore t)
   c_uz_s (chord underscore s)
   c_apos_h (chord apos h)
   c_apos_a (chord apos a)
   c_s_tab (tap-hold $tap-time $hold-time C-S-tab M-left)
   c_tab (tap-hold $tap-time $hold-time C-tab M-right)
   m_tab (tap-hold $tap-time $hold-time M-tab C-A-M-m)
   mc C-up ;; Mission Control
  sls M-spc ;; Spotlight Search
  dtn S-C-A-M-f5 ;; Dictation
  dnd S-C-A-M-f6 ;; Do Not Disturb
  lp S-C-A-M-l ;; Launchpad
)
(defchords underscore 15
  (r    ) @r
  (  t  ) @t
  (    s) @s
  ;; (r t  ) - 
  ;; (  t s) z 
)

(defchords apos 15
  (h  ) @h
  (  a) @a
  ;; (h a) ' 
)


